    <fields>
        <fullName>Hours_sold__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
AND(
ISNULL(Opportunity__r.Hours_in_a_day__c) = False,
Opportunity__r.Hours_in_a_day__c &gt; 0
),
Days_sold__c * Opportunity__r.Hours_in_a_day__c,
Days_sold__c * 7
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hours sold</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
