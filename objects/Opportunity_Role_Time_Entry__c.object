    <validationRules>
        <fullName>Check_ORR_month</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
YEAR(Date__c) &lt;&gt; Opportunity_Role_Record__r.Rec_date_yr__c,
MONTH(Date__c) &lt;&gt; Opportunity_Role_Record__r.Rec_date_mth__c
)</errorConditionFormula>
        <errorDisplayField>Date__c</errorDisplayField>
        <errorMessage>Please make sure you have selected the Opportunity Role Record that is the same month as the Date you have entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORR_remaining_days_check</fullName>
        <active>false</active>
        <errorConditionFormula>Duration__c &gt; Opportunity_Role_Record__r.Remain_Fac_hours_ad_hoc_for_prod_OPE__c</errorConditionFormula>
        <errorDisplayField>Duration__c</errorDisplayField>
        <errorMessage>The duration you have entered is greater than the remaining budget of days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opp_must_be_closed_won</fullName>
        <active>true</active>
        <description>Can&apos;t add time to an Opportunity that hasn&apos;t been won</description>
        <errorConditionFormula>IF(TEXT(Opportunity_Role_Record__r.Opportunity__r.StageName) &lt;&gt; &quot;Closed Won&quot;,TRUE,FALSE)</errorConditionFormula>
        <errorMessage>You cannot add time entries to an Opportunity that has not been Closed / Won</errorMessage>
    </validationRules>
