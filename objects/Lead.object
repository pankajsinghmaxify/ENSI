    <validationRules>
        <fullName>OC_PAX_must_have_parent</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
TEXT(Opportunity_record_type__c) = &quot;OC PAX&quot;,
ISBLANK(Parent_Opportunity__c),
TEXT(Status) = &quot;Converted&quot;
)</errorConditionFormula>
        <errorDisplayField>Parent_Opportunity__c</errorDisplayField>
        <errorMessage>To create an OC PAX Opportunity you must define a parent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Parent_only_for_OC_PAX</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
TEXT(Opportunity_record_type__c) &lt;&gt; &quot;OC PAX&quot;,
ISBLANK(Parent_Opportunity__c) = False
)</errorConditionFormula>
        <errorDisplayField>Parent_Opportunity__c</errorDisplayField>
        <errorMessage>You cannot define a parent for an Opportunity that isn&apos;t an OC PAX. Please empty this field, or change the Opportunity record type.</errorMessage>
    </validationRules>
