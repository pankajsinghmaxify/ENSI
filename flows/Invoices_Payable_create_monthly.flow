    <decisions>
        <name>vIndexMonth_is_CurrentMonth</name>
        <label>vIndexMonth is CurrentMonth</label>
        <locationX>813</locationX>
        <locationY>688</locationY>
        <defaultConnector>
            <targetReference>Iterate_vIndexMonth</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>False</defaultConnectorLabel>
        <rules>
            <name>No_monthlies</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sOpp.Opportunity_Types__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Consulting / retainer</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_scOR</targetReference>
            </connector>
            <label>No monthlies</label>
        </rules>
        <rules>
            <name>vIndexMonth_is_CurrentMonth_true</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>fCurrentDateYear</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>vIndexYear</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fCurrentDateMonth</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>vIndexMonth</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_scOR</targetReference>
            </connector>
            <label>True</label>
        </rules>
        <rules>
            <name>vIndexMonth_is_CurrentMonth_future</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>fOppCloseMonth</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>fCurrentDateMonth</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fOppCloseYear</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>fCurrentDateYear</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_scOR</targetReference>
            </connector>
            <label>Closed date in future</label>
        </rules>
    </decisions>
    <formulas>
        <name>fCurrentDateMonth</name>
        <dataType>Number</dataType>
        <expression>MONTH(TODAY())</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fCurrentDateYear</name>
        <dataType>Number</dataType>
        <expression>YEAR(TODAY())</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fIndexDate</name>
        <dataType>Date</dataType>
        <expression>DATE({!vIndexYear},{!vIndexMonth},28)</expression>
    </formulas>
    <formulas>
        <name>fIndexLabel</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!fIndexDate})) &amp; TEXT(MONTH({!fIndexDate}))</expression>
    </formulas>
    <formulas>
        <name>fIndexMonthPlusOne</name>
        <dataType>Number</dataType>
        <expression>IF({!vIndexMonth}+1=13,1,{!vIndexMonth}+1)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fIndexYearPlusOne</name>
        <dataType>Number</dataType>
        <expression>IF({!fIndexMonthPlusOne}=1,{!vIndexYear}+1,{!vIndexYear})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fInvOwnerId</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!sOpp.Assigned_CSM__c}),{!sOpp.OwnerId},{!sOpp.Assigned_CSM__c})</expression>
    </formulas>
    <formulas>
        <name>fOppCloseMonth</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK({!sOpp.Retainer_start_date__c}),MONTH({!sOpp.CloseDate}),MONTH({!sOpp.Retainer_start_date__c}))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fOppCloseYear</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK({!sOpp.Retainer_start_date__c}),YEAR({!sOpp.CloseDate}),YEAR({!sOpp.Retainer_start_date__c}))</expression>
        <scale>0</scale>
    </formulas>
